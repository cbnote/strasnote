{
	"Serilog": {
		"Using": [ "Serilog.Sinks.Console" ],
		"MinimumLevel": "Verbose",
		"WriteTo": [
			{
				"Name": "Console",
				"Args": {
					"outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} | {SourceContext}{NewLine}{Exception}"
				}
			}
		],
		"Properties": {
			"Application": "Auth.Api"
		}
	},
	"AllowedHosts": "*"
}
